{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "lib": ["es6"],
        "declaration": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "strictNullChecks": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules",
        "examples",
        "test",
        "dist"
    ]
}
